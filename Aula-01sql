CREATE TABLE T_TESTE_DLL (
    CD_TESTE NUMBER(3)    NOT NULL,
    NM_TESTE VARCHAR2(30) NOT NULL,
    DS_TESTE VARCHAR2(60) NULL,
    DT_TESTE DATE         NULL
);

ALTER TABLE T_TESTE_DLL ADD CONSTRAINT PK_TESTE_DLL PRIMARY KEY (CD_TESTE);

ALTER TABLE T_TESTE_DLL ADD CONSTRAINT UN_TESTE_DLL_NOME UNIQUE (NM_TESTE);

ALTER TABLE T_TESTE_DLL ADD DS_EMAIL VARCHAR2(80);

ALTER TABLE T_TESTE_DLL ADD (DS_OBS1 VARCHAR2(50) NOT NULL 
                            ,DS_OBS2 VARCHAR2(50));

ALTER TABLE T_TESTE_DLL MODIFY DS_OBS1 VARCHAR2(80);

ALTER TABLE T_TESTE_DLL MODIFY DS_TESTE NOT NULL;

ALTER TABLE T_TESTE_DLL MODIFY NM_TESTE  NULL;

ALTER TABLE T_TESTE_DLL DROP COLUMN DS_OBS2;

ALTER TABLE T_TESTE_DLL DROP CONSTRAINT PK_TESTE_DLL;

ALTER TABLE T_TESTE_DLL MODIFY DT_TESTE DEFAULT NULL;








